	"author": "Bence Markus<bence.markus@outlook.com>",
	"private": true,
	"repository": {
		"type": "git",
		"url": "git@github.com:mrksbnc/ambinace-core.git"
	},
	"prisma": {
		"schema": "src/database/prisma/ambiance_core.prisma"
	},
	"scripts": {
		"dev": "nodemon",
		"start": "node dist",
		"type-check": "tsc --noEmit",
		"lint:fix": "yarn lint --fix",
		"lint": "eslint . --ext .ts,.js",
		"format": "yarn prettier --write src",
		"format:check": "yarn prettier --check src",
		"build": "rm -rf dist && node scripts/build.js",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"keywords": [

		"esbuild": "^0.17.18",
		"eslint": "^8.40.0",
		"eslint-config-prettier": "^8.8.0",
		"jest": "^29.5.0",
		"jest-mock-extended": "^3.0.4",
		"nodemon": "^2.0.22",
		"npm-dts": "^1.3.12",
		"prettier": "^2.8.8",
		"ts-jest": "^29.1.0",
		"typescript": "^5.0.4"
	}
